  },
  "homepage": "https://github.com/chadhindsight/pro-shop#readme",
  "dependencies": {
    "ajv": "^6.12.5",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.8",
    "node": "^14.13.0",
    "nodemon": "^2.0.4"
